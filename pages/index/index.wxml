<view class="wrap">
  <view class="bd">
    <view class="category" wx:for="{{category}}" wx:key="id" data-category-id="{{item.id}}" bindtap="openListPage">
      <image class="category-bd" src="./images/{{item.id}}.png"></image>
      <text class="category-ft">{{item.name}}</text>
    </view>
  </view>
</view>
